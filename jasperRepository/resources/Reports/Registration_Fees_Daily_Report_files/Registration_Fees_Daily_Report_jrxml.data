	<field name="visit_id" class="java.lang.Integer"/>
	<variable name="Registration_Fees_1" class="java.lang.Double" resetType="Column" calculation="Sum">
		<variableExpression><![CDATA[$F{Registration_Fees}]]></variableExpression>
	</variable>
