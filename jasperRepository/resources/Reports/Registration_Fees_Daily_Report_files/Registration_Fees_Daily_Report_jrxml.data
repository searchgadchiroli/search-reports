	<property name="ireport.y" value="0"/>
	<parameter name="date" class="java.util.Date">
		<defaultValueExpression><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select v.patient_id, pi.identifier, (case when date(p.date_created) =  $P{date} then 'New' else 'Old' end) as Patient_Type,
Locale_Name.name, Locale_Name.mobile,
pa.city_village, p.gender,
sum(coalesce(o.value_numeric,0)) as "Registration_Fees",
v.visit_id
From visit v
inner join person p on v.patient_id = p.person_id
inner join person_address pa on p.person_id = pa.person_id
inner join patient_identifier pi on pi.patient_id = p.person_id
inner join (select pa.person_id, concat(group_concat(case when pat.name = 'givenNameLocal' then pa.value else '' end separator ''), ' ',
group_concat(case when pat.name = 'middleNameLocal' then pa.value else '' end separator ''), ' ',
group_concat(case when pat.name = 'familyNameLocal' then pa.value else '' end separator '')) as name,
group_concat(case when pat.name = 'Primary Contact' then pa.value else '' end separator '') as mobile
from person_attribute as pa
inner join person_attribute_type pat on pa.person_attribute_type_id = pat.person_attribute_type_id
group by pa.person_id) as Locale_Name on p.person_id = Locale_Name.person_id
left outer join encounter e on e.visit_id = v.visit_id
left outer join encounter_type et on e.encounter_type = et.encounter_type_id AND et.name = 'REG'
left outer join obs o on o.encounter_id = e.encounter_id and o.voided = 0
left outer join concept_name as cn on cn.concept_id = o.concept_id AND cn.name = 'REGISTRATION FEES'
where date(v.date_started) = $P{date}
group by v.patient_id, pi.identifier, pa.city_village, p.gender, v.visit_id;]]>
	</queryString>
